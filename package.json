{
  "name": "eslint-cache",
  "version": "1.0.0",
  "description": "support eslint cache",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "eslint-cache": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxfdarling/eslint-cache.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hxfdarling/eslint-cache/issues"
  },
  "homepage": "https://github.com/hxfdarling/eslint-cache#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3"
  },
  "devDependencies": {
    "commitlint-config-imt": "0.0.4",
    "eslint": "^5.12.1",
    "eslint-config-zman": "^0.2.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0"
  },
  "lint-staged": {
    "*.{json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/commitlint-config-imt/cz-config.js"
    }
  }
}
